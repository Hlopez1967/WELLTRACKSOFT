<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - WELLTRACKSOFT</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/WELLTRACKSOFT.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/all.css" />


    <!--bootstrap-->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.3.0/font/bootstrap-icons.css">



    <!--DATATABLES.NET-->

    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/v/dt/dt-1.10.22/datatables.min.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/buttons/1.6.4/css/buttons.dataTables.min.css" />
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.24/css/jquery.dataTables.min.css" />


</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">WELLTRACKSOFT</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    

 <div class="container-fluid">


        <div class="row flex-nowrap">
            <div class="col-auto col-md-1 col-xl-1 px-sm-1 px-0 shadow p-3 bg-white rounded" style="background-color:#f5f5f5">
                <div class="d-flex flex-column align-items-center align-items-sm-center px-3 pt-2 text-white min-vh-100">
                    <ul class="list-group">
                        <li class="list-group-item">
                            <a href="#" class="nav-link align-middle px-0">
                                <img data-v-58e6bbed="" src="https://s3.amazonaws.com/cdn.officepuzzle.com/icons/346-home.svg" width="50" height="50" class="nav-icon">
                                <span data-v-58e6bbed="" class="nav-text mt-2">Home</span>
                            </a>
                        </li>

                        <li class="list-group-item">
                            <a asp-controller="Home" asp-action="Dashboard" class="text-decoration-none">
                                <img data-v-58e6bbed="" src="https://s3.amazonaws.com/cdn.officepuzzle.com/icons/123-dashboard.svg" width="50" height="50" class="nav-icon">
                                <span data-v-58e6bbed="" class="nav-text mt-2">Dashboard</span>
                            </a>
                        </li>


                        <li class="list-group-item">
                            <a href="#" class="nav-link px-0 align-middle">
                                <img src="https://s3.amazonaws.com/cdn.officepuzzle.com/icons/360-users.svg" width="50" height="50" alt="Alternate Text" />
                                <span data-v-58e6bbed="" class="nav-text mt-2">Clients</span>
                            </a>
                        </li>

                        <li class="list-group-item">
                            <a href="#" class="nav-link px-0 align-middle">
                                <img src="https://s3.amazonaws.com/cdn.officepuzzle.com/icons/367-user.svg" width="50" height="50" alt="Alternate Text" />
                                <span data-v-58e6bbed="" class="nav-text mt-2">Users</span>
                            </a>
                        </li>


                        <li class="list-group-item">
                            <a href="#" class="nav-link px-0 align-middle">
                                <img src="https://s3.amazonaws.com/cdn.officepuzzle.com/icons/271-calendar.svg" width="50" height="50" alt="Alternate Text" />
                                <span data-v-58e6bbed="" class="nav-text mt-2">Calendar</span>
                            </a>
                        </li>


                        <li class="list-group-item">
                            <a href="#" class="nav-link px-0 align-middle">
                                <img src="https://s3.amazonaws.com/cdn.officepuzzle.com/icons/364-user.svg" width="50" height="50" alt="Alternate Text" />
                                <span data-v-58e6bbed="" class="nav-text mt-2">Profile</span>
                            </a>
                        </li>

                        <li class="list-group-item">
                            <a href="#" class="nav-link px-0 align-middle">
                                <img src="https://s3.amazonaws.com/cdn.officepuzzle.com/icons/167-file-19.svg" width="50" height="50" alt="Alternate Text" />
                                <span data-v-58e6bbed="" class="nav-text mt-2">Resources</span>
                            </a>
                        </li>

                        <li class="list-group-item">
                            <a href="#" class="nav-link px-0 align-middle">
                                <img src="https://s3.amazonaws.com/cdn.officepuzzle.com/icons/322-exit.svg" width="50" height="50" alt="Alternate Text" />
                                <span data-v-58e6bbed="" class="nav-text mt-2">Exit</span>
                            </a>
                        </li>


                    </ul>


                </div>


            </div>

            <div class="col-auto col-md-10">

                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>
        </div>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - WELLTRACKSOFT - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <!--jquery-->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.4.1/dist/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>



    <!--DATABALES.NET-->

    <script type="text/javascript" src="https://cdn.datatables.net/v/dt/dt-1.10.22/datatables.min.js"></script>
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.4/js/dataTables.buttons.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.4/js/buttons.print.min.js"></script>
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.5.1.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.4/js/dataTables.buttons.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.4/js/buttons.flash.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.4/js/buttons.html5.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.4/js/buttons.print.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/fixedcolumns/4.0.2/js/dataTables.fixedColumns.min.js"></script>


    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
